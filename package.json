{
    "name": "datasphere-backend",
    "version": "0.1.0",
    "description": "Backend API for employee management with auth using MySQL (Hostinger)",
    "main": "src/server.js",
    "scripts": {
        "start": "node src/server.js",
        "dev": "nodemon --config nodemon.json src/server.js",
        "migrate": "node scripts/migrate.js",
        "seed": "node scripts/seedDummyEmployees.js",
        "seed:real": "node scripts/seedRealEmployees.js",
        "setup": "npm run migrate && npm run seed"
    },
    "keywords": [
        "express",
        "mysql",
        "auth",
        "employee"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "helmet": "^7.1.0",
        "jsonwebtoken": "^9.0.2",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "mysql2": "^3.9.9",
        "node-cron": "^4.2.1"
    },
    "devDependencies": {
        "nodemon": "^3.0.3"
    }
}
